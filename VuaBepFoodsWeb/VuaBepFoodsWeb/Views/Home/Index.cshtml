@{
    ViewData["Title"] = "Vua Bếp Foods - Nước sốt ướp thịt nướng đậm đà";
}

@section Scripts{
    <script>
        $(document).ready(function () {

            //Init bootstrap max length
            $('form [maxlength]').maxlength({
                alwaysShow: !0,
            });

            $('#form_data_contact').on('submit', function (e) {
                e.preventDefault();
                e.stopImmediatePropagation();
                let $formElm = $(this);
                let isvalidate = CheckValidationUnobtrusive($formElm);
                if (!isvalidate) { ShowToastNoti('warning', '', _resultActionResource.PleaseWrite); return false; }
                let formData = new FormData($formElm[0]);
                var laddaSubmitForm = Ladda.create(document.querySelector('#btn_submit_form_contact'));
                laddaSubmitForm.start();
                $.ajax({
                    url: '/Home/SubmitContact',
                    type: 'POST',
                    data: formData,
                    contentType: false,
                    processData: false,
                    success: function (response) {
                        laddaSubmitForm.stop();
                        if (!CheckResponseIsSuccess(response)) return false;
                        swal.fire('Đã gửi!', '', 'success');
                        $('#form_data_contact').trigger('reset');
                    }, error: function (err) {
                        laddaSubmitForm.stop();
                        CheckResponseIsSuccess({ result: -1, error: { code: err.status } });
                    }
                });
            });

        });
    </script>
}

